{
  "name": "ruso-monorepo",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "This workspace contains the base of a collection of packages.",
  "author": {
    "name": "Ruslan Gonzalez",
    "email": "ruslanguns@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslanguns/ruso-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/ruslanguns/ruso-monorepo/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "nestjs",
    "angular",
    "javascript",
    "common",
    "utils",
    "tools"
  ],
  "scripts": {
    "ng": "ng",
    "preversion": "yarn run format && yarn run lint && lerna run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "check": "npm-check -u",
    "commit": "git-cz",
    "format": "prettier --write \"packages/**/*.{ts,js,json}\" ",
    "lint": "eslint \"{packages, integration, src,apps,libs,test}/**/*.ts\" --fix",
    "setup": "yarn && yarn build",
    "test": "lerna run test",
    "publish": "lerna publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/animations": "12.2.17",
    "@angular/cli": "12.2.7",
    "@angular/common": "12.2.17",
    "@angular/compiler": "12.2.17",
    "@angular/compiler-cli": "12.2.17",
    "@angular/core": "12.2.17",
    "@angular/language-service": "12.2.17",
    "@angular/platform-browser": "12.2.17",
    "@angular/platform-browser-dynamic": "12.2.17",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@golevelup/nestjs-testing": "0.1.2",
    "@golevelup/ts-jest": "0.3.4",
    "@nestjs/cli": "9.1.5",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/schematics": "9.0.3",
    "@types/express": "4.17.14",
    "@types/jest": "27.5.2",
    "@types/node": "18.11.13",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "codelyzer": "6.0.2",
    "conventional-changelog-cli": "2.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "husky": "7.0.2",
    "jest": "27.5.1",
    "jest-preset-angular": "10.1.0",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "ng-packagr": "12.2.2",
    "npm-check": "5.9.2",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "snyk": "1.1061.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
    "tsickle": "0.43.0",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5",
    "zone.js": "0.12.0"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,html,json,js}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "minimist": "^1.2.2",
    "@types/express/@types/express-serve-static-core": "4.17.24",
    "axios": "^0.21.0"
  },
  "dependencies": {
    "@angular/forms": "12.2.17",
    "@ruso/common": "^0.0.2",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "faker": "5.5.3",
    "figlet": "1.5.2",
    "inquirer": "8.1.5",
    "socket.io-client": "4.2.0",
    "yargs": "17.2.1"
  }
}
