{
  "name": "ruso-monorepo",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "This workspace contains the base of a collection of packages.",
  "author": {
    "name": "Ruslan Gonzalez",
    "email": "ruslanguns@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslanguns/ruso-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/ruslanguns/ruso-monorepo/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "nestjs",
    "angular",
    "javascript",
    "common",
    "utils",
    "tools"
  ],
  "scripts": {
    "preversion": "yarn run format && yarn run lint && lerna run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "check": "npm-check -u",
    "commit": "git-cz",
    "format": "prettier --write \"packages/**/*.{ts,js,json}\"",
    "lint": "eslint --ext .ts .",
    "setup": "yarn && yarn build",
    "test": "lerna run test",
    "test:watch": "lerna run test:watch",
    "test:cov": "lerna run test:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.14",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/animations": "10.2.5",
    "@angular/cli": "10.2.3",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/compiler-cli": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@golevelup/nestjs-testing": "0.1.2",
    "@golevelup/ts-jest": "0.3.1",
    "@nestjs/cli": "8.1.1",
    "@nestjs/common": "8.0.7",
    "@nestjs/core": "8.0.7",
    "@nestjs/microservices": "8.0.7",
    "@nestjs/platform-express": "8.0.7",
    "@nestjs/platform-fastify": "8.0.7",
    "@nestjs/platform-socket.io": "8.0.7",
    "@nestjs/platform-ws": "8.0.7",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/websockets": "8.0.7",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.19",
    "@types/socket.io": "3.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "conventional-changelog-cli": "2.1.1",
    "codelyzer": "6.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "eslint-plugin-prettier": "3.4.1",
    "express": "4.17.1",
    "fastify": "3.21.6",
    "husky": "4.3.8",
    "jest": "26.4.2",
    "jest-preset-angular": "8.4.0",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "ng-packagr": "10.1.2",
    "npm-check": "5.9.2",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "snyk": "1.398.1",
    "supertest": "4.0.2",
    "ts-jest": "26.3.0",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsickle": "0.43.0",
    "tslib": "2.0.1",
    "tslint": "6.1.3",
    "typescript": "4.4.3",
    "zone.js": "0.11.4"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext ts"
    ],
    "*.{md,html,json,js}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "minimist": "^1.2.2",
    "@types/express/@types/express-serve-static-core": "4.17.24",
    "axios": "^0.21.0"
  },
  "dependencies": {
    "@angular/forms": "10.2.5",
    "@ruso/common": "^0.0.1",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "faker": "5.5.3",
    "figlet": "1.5.2",
    "inquirer": "7.3.3",
    "yargs": "16.0.3"
  }
}
