{
  "name": "ruso-monorepo",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "This workspace contains the base of a collection of packages.",
  "author": {
    "name": "Ruslan Gonzalez",
    "email": "ruslanguns@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslanguns/ruso-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/ruslanguns/ruso-monorepo/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "nestjs",
    "angular",
    "javascript",
    "common",
    "utils",
    "tools"
  ],
  "scripts": {
    "ng": "ng",
    "preversion": "yarn run format && yarn run lint && lerna run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "check": "npm-check -u",
    "commit": "git-cz",
    "format": "prettier --write \"packages/**/*.{ts,js,json}\" ",
    "lint": "eslint \"{packages, integration, src,apps,libs,test}/**/*.ts\" --fix",
    "setup": "yarn && yarn build",
    "test": "lerna run test",
    "publish": "lerna publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.1.0",
    "@angular-devkit/build-angular": "15.0.3",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/animations": "15.0.3",
    "@angular/cli": "15.0.3",
    "@angular/common": "15.0.3",
    "@angular/compiler": "15.0.3",
    "@angular/compiler-cli": "15.0.3",
    "@angular/core": "15.0.3",
    "@angular/language-service": "15.0.3",
    "@angular/platform-browser": "15.0.3",
    "@angular/platform-browser-dynamic": "15.0.3",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@golevelup/nestjs-testing": "0.1.2",
    "@golevelup/ts-jest": "0.3.4",
    "@nestjs/cli": "9.1.5",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/schematics": "9.0.3",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.13",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "codelyzer": "6.0.2",
    "conventional-changelog-cli": "2.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-preset-angular": "^12.2.3",
    "lerna": "6.1.0",
    "lint-staged": "13.1.0",
    "ng-packagr": "15.0.3",
    "npm-check": "6.0.1",
    "prettier": "2.8.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "snyk": "1.1069.0",
    "supertest": "6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsickle": "0.46.3",
    "tslib": "2.4.1",
    "tslint": "6.1.3",
    "typescript": "4.8.2",
    "zone.js": "0.12.0"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,html,json,js}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "minimist": "^1.2.2",
    "@types/express/@types/express-serve-static-core": "4.17.24",
    "axios": "^1.0.0"
  },
  "dependencies": {
    "@angular/forms": "15.0.3",
    "@ruso/common": "^0.1.1",
    "@types/express-serve-static-core": "4.17.31",
    "chalk": "5.2.0",
    "clear": "0.1.0",
    "faker": "6.6.6",
    "figlet": "1.5.2",
    "inquirer": "9.1.4",
    "minimist": "^1.2.7",
    "socket.io-client": "^4.5.4",
    "yargs": "17.6.2"
  }
}
